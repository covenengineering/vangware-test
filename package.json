		"eslint-plugin-prettier": "^3.4.0",
		"husky": "4.3.8",
		"npm-run-all": "^4.1.5",
		"prettier": "2.3.2",
		"typedoc": "^0.21.4",
		"typescript": "4.3.5"
	},
	"exports": {
		"import": "./dist/import/index.js",
		"require": "./dist/require/index.js"
	},
	"files": [
		"dist"
	],
	"homepage": "https://test.vangware.com",
	"husky": {
		"hooks": {
			"pre-commit": "run-s clean pre-compile lint test docs"
		}
	},
	"keywords": [
		"legibility",
		"legible",
		"test",
		"testing",
		"tests",
		"vangware"
	],
	"license": "MIT",
	"repository": "vangware/test",
	"scripts": {
		"clean": "rimraf ./dist",
		"compile": "run-s compile:*",
		"compile:import": "tsc --project tsconfig.import.json",
		"compile:require": "tsc --project tsconfig.require.json",
		"docs": "typedoc --disableOutputCheck --gitRevision main --theme minimal ./src/index.ts",
		"lint": "eslint {src,tests}/**/*.ts",
		"lint:fix": "eslint {src,tests}/**/*.ts --fix",
		"pre-compile": "run-s pre-compile:*",
		"pre-compile:import": "tsc --noemit --project tsconfig.import.json",
		"pre-compile:require": "tsc --noemit --project tsconfig.require.json",
		"prepublishOnly": "run-s compile prettify",
		"prettify": "prettier --write --loglevel warn './dist/**/*.{js,ts}'",
		"test": "c8 --reporter=html --reporter=text-summary --reporter=lcov ts-node ./tests"
	},
	"types": "./dist/import/index.d.ts"
}
